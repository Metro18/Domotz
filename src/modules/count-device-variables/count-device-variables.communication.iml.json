{
	"url": "/agent/{{parameters.agent_id}}/device/{{parameters.device_id}}/variable",
	"method": "HEAD",
	"headers": {
		"X-Api-Key": "{{connection.apiKey}}"
	},
	"qs": {
		"value": "{{parameters.value}}",
		"path": "{{parameters.path}}",
		"has_history": "{{parameters.has_history}}",
		"metric": "{{parameters.metric}}"
	},
	"response": {
		"output": {
			"count": "{{headers.\"X-Entities-Count\"}}"
		}
	}
}
