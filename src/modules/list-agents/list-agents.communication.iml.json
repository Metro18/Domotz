{
	// Request to API endpoint with parameter "id" defined in Mappable parameters.
	"url": "/agent",      // Relative to base URL
	"method": "GET",
	"qs": {
		"page_size": "{{min(100, parameters.limit)}}",  // API max is 100
		"page_number": "{{pagination.page - 1}}",  // API uses 0-indexed pages
		"display_name": "{{parameters.display_name}}",  // Parameter "display_name" that is defined in Mappable parameters.
		"team_name": "{{parameters.team_name}}"  // Parameter "team_name" that is defined in Mappable parameters.
	}, // Query string

	// Response handling
	"response": {
		"limit": "{{parameters.limit}}",
		"iterate": "{{body}}",
		"output": "{{item}}"
	},

	// Pagination
	"pagination": {
		"qs": {
			"page_number": "{{pagination.page - 1}}"
		},
		"condition": "{{length(body) > 0}}"
	}
}